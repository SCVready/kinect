    "version": "0.2.0",
    "configurations": [
        {
            "name": "Degub native UT",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/test/build/kinect_tests",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/test/build/",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "type": "gdb",
            "request": "launch",
            "name": "Launch Program (SSH)",
            "target": "build/kinectalarm",
            "cwd": "${workspaceRoot}",
            "pathSubstitutions": {
                "/home/scvready/workspace/github/presenceos/kinectalarm/": "/home/root/kinectalarm/"
            },
            "ssh": {
                "host": "192.168.0.18",
                "cwd": "/home/root/kinectalarm",
                "user": "root",
                "password": "",
                "useAgent": false,
                "forwardX11": false
            },
            "preLaunchTask": "sync"
        },
        
    ]
}